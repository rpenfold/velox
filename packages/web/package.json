{
  "name": "xl-formula-web",
  "version": "1.0.0",
  "description": "High-performance Excel-like formula parser and evaluator for web applications",
  "type": "module",
  "main": "xl-formula-wrapper.js",
  "module": "xl-formula-wrapper.js",
  "types": "xl-formula.d.ts",
  "files": [
    "xl-formula.js",
    "xl-formula.wasm",
    "xl-formula-wrapper.js",
    "xl-formula.d.ts",
    "README.md"
  ],
  "scripts": {
    "build": "cd ../.. && ./build.sh --web",
    "clean": "rm -f xl-formula.js xl-formula.wasm",
    "prepublishOnly": "npm run build",
    "test": "node --experimental-vm-modules ../../node_modules/.bin/jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testMatch": ["<rootDir>/tests/**/*.test.js"],
    "testTimeout": 30000,
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
    "transform": {},
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "keywords": [
    "excel",
    "formula",
    "parser",
    "evaluator",
    "webassembly",
    "wasm",
    "spreadsheet",
    "calculation",
    "math",
    "javascript",
    "typescript"
  ],
  "author": "XL Formula Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/xl-formula.git",
    "directory": "web"
  },
  "bugs": {
    "url": "https://github.com/your-username/xl-formula/issues"
  },
  "homepage": "https://github.com/your-username/xl-formula#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "exports": {
    ".": {
      "import": "./xl-formula-wrapper.js",
      "require": "./xl-formula-wrapper.js",
      "types": "./xl-formula.d.ts"
    },
    "./package.json": "./package.json"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}