{
  "name": "@velox/formulas",
  "version": "1.0.0",
  "description": "High-performance formula parser and evaluator for web applications",
  "type": "module",
  "main": "formula-wrapper.js",
  "module": "formula-wrapper.js",
  "types": "formulas.d.ts",
  "files": [
    "formulas.js",
    "formulas.wasm",
    "formula-wrapper.js",
    "formulas.d.ts",
    "README.md"
  ],
  "scripts": {
    "build": "cd ../.. && ./build.sh --web",
    "clean": "rm -f formulas.js formulas.wasm",
    "prepublishOnly": "npm run build",
    "test": "node --experimental-vm-modules ../../node_modules/.bin/jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testMatch": ["<rootDir>/tests/**/*.test.js"],
    "testTimeout": 30000,
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
    "transform": {},
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "keywords": [
    "velox",
    "formula",
    "parser",
    "evaluator",
    "webassembly",
    "wasm",
    "spreadsheet",
    "calculation",
    "math",
    "javascript",
    "typescript"
  ],
  "author": "Velox Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/velox.git",
    "directory": "packages/web"
  },
  "bugs": {
    "url": "https://github.com/your-username/velox/issues"
  },
  "homepage": "https://github.com/your-username/velox#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "exports": {
    ".": {
      "import": "./formula-wrapper.js",
      "require": "./formula-wrapper.js",
      "types": "./formulas.d.ts"
    },
    "./package.json": "./package.json"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}